--game loop
function _init()
 rain_init()
 p_init()
 sk_init()
 coin_init()

 gravity=.2
 friction=.85

 palt(0,false)
 palt(2,true)

 rx1=0 rx2=0
 ry1=0 ry2=0

 cam_x=0
 map_start=0
 map_end=1024
end

function _update60()
 p_update()
 p_animate()
 rain_update()
 sk_update()
 sk_animate()
 cam_update()
 coin_animate()
end

function _draw()
 cls(1)
 rain_draw()
 map(0,0)
 coin_draw()
 p_draw()
 sk_draw()
end
