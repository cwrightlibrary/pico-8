--coin
function coin_init()
 coin={
  sp=181,
  x=2,
  y=7,
  w=8,
  h=8,
  shimmer=0,
  no_shimmer_length=flr(rnd(8))+3,
  anim=0
 }
end
function coin_animate()
 coin.sp+=.15
 if coin.sp>=184
 and coin.shimmer<coin.no_shimmer_length then
  coin.shimmer+=1
  coin.sp=181
 end
 if coin.sp>188
 and coin.shimmer==coin.no_shimmer_length then
  coin.sp=181
  coin.shimmer=0
  coin.no_shimmer_length=flr(rnd(8))+3
 end
end

function coin_draw()
 spr(coin.sp,coin.x*8,coin.y*8,coin.w/8,coin.h/8)
end
