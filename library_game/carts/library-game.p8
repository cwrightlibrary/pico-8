   switch=false,
   is_waiting=false,
   forward=false,
   falling=false,
   landed=true,
   boost=3,
   hit=false
  }
  add(sk_small_index,sk_small)
 elseif obj_name=='q_brick' then
  local q_brick={
   sp=138,
   x=objx,
   y=objy,
   w=objw,
   h=objh,
   anim=0,
   hold=0,
   run=false,
