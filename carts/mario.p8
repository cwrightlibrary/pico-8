function _init()
  player={
    sp=0,
    x=2,
    y=90,
    w=8,
    h=8,
    sp_w=1,
    sp_h=1,
    flp=false,
    dx=0,
    dy=0,
    max_dx=1.85,
    max_dy=5,
    acc=0.43,
    boost=5.4,
    anim=0,
    running=false,
    jumping=false,
    falling=false,
    landed=false,
    jump_snd=1
  }
  
  coin={
   sp=35,
   x=15,
   y=8,
   anim=0,
   snd=0
  }
  
  goomba={
   sp=3,
   x=10,
   y=12,
   w=8,
   h=8,
   speed=.5/8,
   flp=true,
   squashed=false,
   anim=0,
   move_right=false
  }

  red_mush={
    sp=56,
    x=20,
    y=8,
    flp=false,
    anim=0
  }

  green_mush={
    sp=57,
    x=21,
    y=4,
    flp=false,
    anim=0
  }

  gravity=0.4
  friction=.7

  --simple camera
  cam_x=0

  ------test------
  x1r=0 y1r=0 x2r=0 y2r=0
  collide_l="no"
  collide_r="no"
  collide_u="no"
  collide_d="no"
  ----------------
  --map limits
  map_start=0
  map_end=1024

  xprint=0
end

