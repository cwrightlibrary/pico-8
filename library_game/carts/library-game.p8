--skeleton
function sk_init()
 sk={
  sp=64,
  flp=false,
  x=8,
  y=13,
  w=16,
  h=16,
  dx=0,
  dy=0,
  max_dx=.05,
  max_dy=4,
  anim=0,
  hold=0,
  switch=false,
  is_waiting=false,
  forward=false,
  falling=false,
  landed=true,
  boost=3
 }
end

function sk_update()
 if not sk.switch then
  if sk.hold<100 then
   sk.hold+=1
  else
   sk.hold=0
   sk.is_waiting=true
  end
 else
  if sk.hold<100 then
   sk.hold+=1
  else
   sk.hold=0
   sk.is_waiting=false
  end
 end
 if sk.is_waiting then
  sk.flp=true
  sk.dx-=.005
  sk.switch=not sk.switch
 else
  sk.flp=false
  sk.dx+=.005
  sk.switch=not sk.switch
 end
 if sk.dx>0 
 or sk.dx<0 then
  sk.dx=limit_speed(sk.dx,sk.max_dx)
 end
 sk.x+=sk.dx
end

function sk_animate()
 if time()-sk.anim>.1 
 and abs(sk.dx)>.02 then
  sk.anim=time()
  if sk.sp==64 then
   sk.forward=not sk.forward
  end
  if sk.sp==72 then
   sk.forward=not sk.forward
  end
  if sk.forward then
   sk.sp+=2
  else
   sk.sp-=2
  end
 end
end

function sk_draw()
 spr(sk.sp,sk.x*8,sk.y*8,sk.w/8,sk.h/8,sk.flp)
end
