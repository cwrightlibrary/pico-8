--game loop
function _init()
 rain_init()
 p_init()
 --sk_init()
 coin_index={}
 q_brick_index={}
 p_brick_index={}
 platform_searched=false

 sk_index={}
 sk_small_index={}

 gravity=.2
 friction=.85

 palt(0,false)
 palt(2,true)

 rx1=0 rx2=0
 ry1=0 ry2=0

 cam_x=0
 map_start=0
 map_end=1024
 success=false
end

function _update60()
 p_update()
 p_animate()
 rain_update()
 sk_update()
 sk_animate()
 sk_small_update()
 sk_small_animate()
 cam_update()
 coin_update()
 q_brick_update()
end

function _draw()
 cls(1)
 rain_draw()
 map(0,0)
 fix_platform()
 add_coin()
 add_brick_coin()
 add_q_brick()
 add_p_brick()
 add_sk()
 add_sk_small()
 p_draw()
 print('score: '..p.score,cam_x,0,10)
end
