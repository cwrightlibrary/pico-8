   end
   sk_small.x+=sk_small.dx
   if object_collide(p.x+3,p.y+1,p.x+3+p.w-6,p.y+1+p.h-1,sk_small.x+3,sk_small.y+3,sk_small.x+3+sk_small.w-6,sk_small.y+3+sk_small.h-3) then
