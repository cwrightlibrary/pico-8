--goomba
function goomba_animate()
 if time()-goomba.anim>.1 then
  goomba.anim=time()
  goomba.sp+=1
  if goomba.sp<3 then
   goomba.sp=3
  end
  if goomba.sp>6 then
   goomba.sp=3
  end
 end
 if goomba.squashed then
  goomba.sp=7
 end
end

function goomba_update()
 --left
 if fget(mget(goomba.x-.1,goomba.y),1) then
  goomba.move_right=true
  goomba.flp=false
  --right
 elseif fget(mget(goomba.x+(goomba.w/8),goomba.y),1) then
  goomba.move_right=false
  goomba.flp=true
 end

 if fget(sget(goomba.x*8,goomba.y*7+4),4)
 or fget(sget(goomba.x*8+2,goomba.y*7+4),4)
 or fget(sget(goomba.x*8+4,goomba.y*7+4),4)
 or fget(sget(goomba.x*8+6,goomba.y*7+4),4) then
  goomba.squashed=true
 end

 if goomba.move_right and not goomba.squashed then
  goomba.x+=goomba.speed
 elseif not goomba.move_right and not goomba.squashed then
  goomba.x-=goomba.speed
 end
end
